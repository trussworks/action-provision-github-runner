  "dependencies": {
    "@actions/artifact": "^0.5.2",
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0",
    "aws-sdk": "^2.966.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.32.0",
    "jest": "^27.0.6"
  }
